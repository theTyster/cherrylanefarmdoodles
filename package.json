{
  "name": "cherrylanefarmdoodles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -fdr .next/ && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --no-cache ",
    "test": "vitest",
    "kv-clear": "./scripts/kv-clear",
    "kv-clear-local": "rm -fdr .wrangler/state/v3/kv",
    "pages:build": "npx @cloudflare/next-on-pages --outdir .vercel/output/static",
    "preview": "npm run pages:build && npx wrangler pages dev .vercel/output/static",
    "deploy": "npm run pages:build && npx wrangler pages deploy .vercel/output/static",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "dependencies": {
    "criptoe": "^1.1.4-0",
    "gsap": "^3.12.5",
    "next": "^15.0.3",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.5",
    "thetyster-utils": "^2.1.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/vitest-pool-workers": "^0.5.27",
    "@cloudflare/workers-types": "^4.20241112.0",
    "@gsap/react": "^2.1.1",
    "@svgr/cli": "^8.1.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^8",
    "eslint-config-next": "^15.0.3",
    "eslint-plugin-next-on-pages": "^1.13.5",
    "gsap": "^3.12.5",
    "npm-check-updates": "^17.1.11",
    "sass": "^1.80.7",
    "typescript": "^5.6.3",
    "vercel": "^39.0.0",
    "vitest": "^2.0.5",
    "wrangler": "^3.86.1"
  }
}
